<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="team2beat.com.qrcodes.QR_and_Register_Activity$PlaceholderFragment">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="379dp"
        android:id="@+id/listView2"
        android:choiceMode="multipleChoice"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/BackToQR2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back to Staff View"
        android:id="@+id/BackToStaff"
        android:onClick="BackToStaff"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/listView2"
        android:layout_alignEnd="@+id/listView2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generate QR and Register"
        android:onClick="BackToQR"
        android:id="@+id/BackToQR2"
        android:layout_gravity="bottom"
        android:layout_above="@+id/BackToStaff"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/listView2"
        android:layout_alignEnd="@+id/listView2" />

</RelativeLayout>
